{
  "name": "uncoverage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf bundle*.js coverage.json",
    "coverage": "browserify app.js -p browserify-derequire -t babelify -o bundle.unins.js && istanbul instrument bundle.unins.js > bundle.js",
    "build": "browserify app.js --debug -t babelify -g [ uglifyify --screw-ie8 --mangle [ --toplevel --eval ] --compress [ --unsafe ] ] | exorcist bundle.js.map > bundle.js",
    "start": "node server",
    "uncoverage": "browserify bundle.unins.js -t ./transform -g [ uglifyify --screw-ie8 --mangle [ --toplevel --eval ] --compress [ --unsafe ] ] -o bundle.min.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "acorn": "^1.0.3",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "browserify-derequire": "^0.9.4",
    "domready": "^1.0.8",
    "escodegen": "^1.6.1",
    "express": "^4.12.3",
    "istanbul-middleware": "^0.2.0",
    "portscanner": "^1.0.0",
    "react": "^0.13.2",
    "through": "^2.3.7",
    "traverse": "^0.6.6",
    "uglifyify": "^3.0.1"
  },
  "es6": true
}
